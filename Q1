class Compute 
{
    static pair getMinMax(long a[], long n)  
    {
        long min=a[0];
        long max=a[0];
        for(int i =0; i<n;i++){
            
            if(max<a[i])
                max=a[i];
                
            if(min>a[i])
                 min=a[i];
        }
        return new pair(min,max);
        }
    }
